    "react-responsive": "^8.1.0",
    "react-scripts": "3.4.1",
    "typescript": "^3.7.5",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --watchAll=false --passWithNoTests --coverage --coverageDirectory=output/coverage/jest",
    "build": "react-app-rewired build",
    "profile": "react-app-rewired build --profile",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint --fix */**/*.{js,ts,tsx}"
  },
  "engines": {
    "node": ">=12.16.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint"
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.10.1",
