    "eslint-plugin-react-hooks": "^4.0.2",
    "husky": "^4.2.5",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "react-app-rewire-aliases": "^0.2.0",
    "react-app-rewired": "^2.1.6",
